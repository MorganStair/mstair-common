# File: .editorconfig

root = true

[*]
charset = utf-8
insert_final_newline = true
max_line_length = 101
trim_trailing_whitespace = true

[*.py]
indent_style = space
indent_size = 4

[*.{json,jsonc}]
indent_style = space
indent_size = 4

[*.{yml,yaml}]
indent_style = space
indent_size = 4

[*.md]
trim_trailing_whitespace = false

[*.{cmd,bat}]
end_of_line = crlf

# End of file: .editorconfig
